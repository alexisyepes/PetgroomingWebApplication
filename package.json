{
  "name": "mern",
  "version": "1.0.0",
  "description": "Amazing Pet Grooming",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Alexis Y Sanabria",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^5.0.0",
    "bcrypt-nodejs": "0.0.3",
    "bcryptjs": "^2.4.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^1.3.3",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "express": "^4.16.3",
    "express-mysql-session": "^2.1.0",
    "express-session": "^1.16.2",
    "express-validator": "^6.1.1",
    "heroku-ssl-redirect": "0.0.4",
    "http-errors": "^1.7.3",
    "if-env": "^1.0.4",
    "jsonwebtoken": "^8.5.1",
    "mdbreact": "^4.16.0",
    "morgan": "^1.9.1",
    "mysql": "^2.17.1",
    "mysql2": "^1.6.5",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-roles": "^1.0.1",
    "query-string": "^6.8.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-responsive-carousel": "^3.1.49",
    "react-router-dom": "^5.0.0",
    "react-scroll-to": "^3.0.0-beta.2",
    "react-scroll-up-button": "^1.6.4",
    "react-select": "^3.0.8",
    "sequelize": "^5.18.0",
    "timeago.js": "^4.0.0-beta.2"
  }
}
